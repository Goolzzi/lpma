{
  "extends": ["prettier",  "plugin:react/recommended", "plugin:jsx-a11y/recommended", "eslint:recommended"],
  "plugins": ["jsx-a11y", "prettier", "react"],
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "prettier/prettier": "error",
    "react/prop-types": [1]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "graphql": true,
    "require" : true,
     "module": false
  }
}
